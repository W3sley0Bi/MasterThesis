@prefix ex: <https://example.org/> .
@prefix foaf1: <https://xmlns.com/foaf/0.1/> .
@prefix rdf1: <https://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs1: <https://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .

ex:hasCar a rdf1:Property ;
    rdfs1:comment "A property linking an employee to their car." ;
    rdfs1:domain ex:Employee ;
    rdfs1:label "hasCar" ;
    rdfs1:range schema:Car .

ex:Employee a rdfs1:Class ;
    rdfs1:comment "A class representing an employee." ;
    rdfs1:label "Employee" ;
    rdfs1:subClassOf foaf1:Person .

